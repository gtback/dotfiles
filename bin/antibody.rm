#!/bin/bash

set -euo pipefail

for var in "$@"; do
    filename="${ANTIBODY_HOME}/https-COLON--SLASH--SLASH-github.com-SLASH-$(echo $var | sd -- '/' '-SLASH-')"

    if [ -d "${filename}" ]; then
        echo "Removing: ${filename}"
        rm -rf "${filename}"
    else
        echo "Does not exist: ${filename}"
    fi

done
